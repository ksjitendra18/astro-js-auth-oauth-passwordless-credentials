---
import MainLayout from "../layout/main-layout.astro";

const isUserLoggedIn = !!Astro.locals.userId;
if (!isUserLoggedIn) {
  return Astro.redirect(`/login?redirect=/dashboard`);
}
---

<MainLayout title="Dashboard" description="dashboard page">
  <h1 class="my-10 text-center">This is a protected route</h1>
</MainLayout>
